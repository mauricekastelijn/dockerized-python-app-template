{
    "name": "Python Dev Container",
    "build": {
      "context": "..",
      "dockerfile": "../Dockerfile"
    },
    "customizations": {
      "vscode": {
        "settings": {
          "python.defaultInterpreterPath": "/usr/local/bin/python"
        },
        "extensions": [
          "ms-python.python",
          "ms-python.vscode-pylance",
          "ms-python.pylint"
        ]
      }
    },
    "remoteUser": "appuser"
  }
  